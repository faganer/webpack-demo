{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "plugins": ["lodash", "html"],
    "extends": ["eslint:recommended", "plugin:lodash/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [1, "single"],
        "semi": [2, "never"]
    },
    "globals": {
        "_": true,
        "Swal": true
    }
}
